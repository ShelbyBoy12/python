    "from math import factorial\n",
    "def fact(n) :\n",
    "    if n == 1 :\n",
    "        return 1\n",
    "    return n * fact(n-1)\n",
    "\n",
    "a = factorial(100)\n",
    "s = 0\n",
    "for digit in str(a) :\n",
    "    s += int(digit)\n",
    "print(s)"
   ]
